{
    "name": "ashraam/license-generator",
    "description": "This package allows you to easily generate license keys with custom templates",
    "keywords": [
        "ashraam",
        "license-generator"
    ],
    "homepage": "https://github.com/ashraam/license-generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Romain BERTOLUCCI",
            "email": "romain.bertolucci@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.23",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ashraam\\LicenseGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ashraam\\LicenseGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
